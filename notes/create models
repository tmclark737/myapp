rails generate model Article title:string text:text

rails generate model Utility_rate sch_name:string peak_start:timestamp peak_stop:timestamp part_start:timestamp part_stop:timestamp customer_charge:integer summer_energy_peak:integer summer_energy_part:integer summer_energy_off:integer winter_energy_part:integer winter_energy_off:integer summer_demand:integer winter_demand:integer pdp_energy:integer pdp_demand:integer


rails generate model Time_table name:string w_day_start:datetime w_day_stop:datetime w_end_start:datetime w_end_stop:datetime note:text


timestamp format
1970-01-01 00:00:01

timestamp test
2000-01-01 00:00:01




schedule creator:

schedule = Schedule.create(name:"M-F 8-5", w_day_begin:"2010-01-01 08:00:00", w_day_stop: "2010-01-01 17:00:00")

